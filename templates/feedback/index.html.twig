{% extends 'base.html.twig' %}

{% block title %}Feedback index{% endblock %}
{% block content %}
{% block body %}
    <h1>Feedback index</h1>
<div class="filter">
</div>
    <table class="table">
        <thead>
            <tr>
                <th>IdF</th>
                <th>Objet</th>
                <th>Text</th>
                <th>Avis</th>
             <!--   <th>actions</th> -->
            </tr>
        </thead>
        <tbody>
        {% for feedback in feedback %}
            <tr>
                <td>{{ feedback.idF }}</td>
                <td>{{ feedback.objet }}</td>
                <td>{{ feedback.text }}</td>
                <td>{{ feedback.avis }}</td>
             <!--   <td>
                    <a href="{{ path('feedback_show', {'idF': feedback.idF}) }}">show</a>
                    <a href="{{ path('feedback_edit', {'idF': feedback.idF}) }}">edit</a>
                </td> -->
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

