{% extends 'baseF.html.twig' %}

{% block title %}Partenaire index{% endblock %}
{% block contentfr %}
    {% block body %}
        <main>
            <!--? slider Area Start-->
            <section class="slider-area slider-area2">
                <div class="slider-active">
                    <!-- Single Slider -->

                </div>
            </section>
            <h1>Partenaire index</h1>

            <table class="table">
                <thead>
                <tr>
                    <th>IdP</th>
                    <th>NomP</th>
                    <th>Domaine</th>
                    <th>DateP</th>
                    <th>Mailp</th>
                    <th>Logop</th>
                    <th>actions</th>
                </tr>
                </thead>
                <tbody>
                {% for partenaire in partenaires %}
                    <tr>
                        <td>{{ partenaire.idP }}</td>
                        <td>{{ partenaire.nomP }}</td>
                        <td>{{ partenaire.domaine }}</td>
                        <td>{{ partenaire.dateP }}</td>
                        <td>{{ partenaire.mailp }}</td>
                        <td>{{ partenaire.logop }}</td>
                        <td>
                            <a href="{{ path('partenaire_show', {'idP': partenaire.idP}) }}">show</a>
                            <a href="{{ path('partenaire_edit', {'idP': partenaire.idP}) }}">edit</a>
                        </td>
                    </tr>
                {% else %}
                    <tr>
                        <td colspan="7">no records found</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>

            <a href="{{ path('partenaire_new') }}">Create new</a>
        </main>
    {% endblock %}
{% endblock %}